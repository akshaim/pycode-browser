from random import choice

def pick_number(from=1,to=10):
	return choice(range(from,to+1))

def ask_question(first,second):
	return input("What's ",first," times ",second '? ')


questions = 10
right=0

while questions <>0:
	first=pick_number()
	second=pick_number()
	ans=ask_question(first,second)
	rans=first*second
	if ans==rans:
		print "That's right : well done"
		right+=1
	else:
		print "Oh sorry, the right answer is :",rans

